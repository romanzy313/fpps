{
  "name": "fpps",
  "description": "Free Peer-to-Peer file Sharing application (Real name TBD)",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --mode development --host",
    "dev:test": "vitest",
    "build:web": "vite build",
    "test": "vitest --run --coverage",
