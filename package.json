    "preact-iso": "^2.11.0",
    "preact-render-to-string": "^6.6.3",
    "streamsaver": "^2.0.6",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@notwoods/eslint-config-preact": "^1.0.0",
    "@preact/preset-vite": "^2.10.2",
    "@types/node": "^25.0.10",
    "@types/streamsaver": "^2.0.5",
    "@vitejs/plugin-basic-ssl": "^2.1.4",
    "@vitest/coverage-v8": "4.0.14",
